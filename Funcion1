int power(int x, unsigned int y)
{
    int temp;
    if( y == 0)
        return 1;
    temp = power(x, y/2);
    if (y%2 == 0)
        return temp*temp;
    else
        return x*temp*temp;
}

double raiz(double n)
{
    return power(n,.5);
}



int main(){
  int j=0;
  int i=0;

  char V[N];
  //llenarArreglo(V);

  double arreglo[7],sum;
  arreglo[0]=4; 
  arreglo[1]=7;
  arreglo[2]=2;
  arreglo[3]=3;
  arreglo[4]=8;
  arreglo[5]=1;
  arreglo[6]=5;
  //double sum =0;
  
  //Funci√≥n
  for(j=0; j<7;j++){
    sum = sum + sqrt(pow(arreglo[j],arreglo[j]));
  }

  printf("\n sum: %f\n",sum);

  return 0;
}
