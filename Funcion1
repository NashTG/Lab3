#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <malloc.h>

#define N 1000000

int V[N];   //Arreglo Global
//lectura de archivo y llenado de V[N]
int insertarArreglo(){
  int i=0;
  int numero=0;

  FILE *archivo;
  char caracter[N];
  archivo = fopen("entrada.txt","r");

  if (archivo == NULL){
    printf("\nError de apertura del archivo. \n\n");
  }else{
    while (fscanf(archivo,"%[^\n]%*[\n]",caracter) != EOF){
      if (1 == sscanf(caracter,"%d",&numero)){
        V[i]=numero;
        i++;
      }
    }
  }
  fclose(archivo);
  return i;
}

int main(){
  int j=0;
  int i=0;
  double sum =0;    //sumatoria
  i =insertarArreglo();   //largo de V[N]
  
  //Funci√≥n Sumatoria
  for(j=0; j<i;j++){
    sum = sum + sqrt(pow(V[j],V[j]));
  }
  printf("El resultado es: %f\n",sum);
  return 0;
}
